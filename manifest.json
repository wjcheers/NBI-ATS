{
  "name": "NBI ATS",
  "version": "0.26",
  "description": "Finds link and email address in the web page",
  "icons": { "128": "jecho.png" },
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    { "matches": ["*://*/*"], "js": ["content_script.js"], "run_at": "document_end" }
  ],
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "*://*.liepin.com/*",
    "*://*.maimai.cn/*",
    "*://*.linkedin.com/*",
    "*://*.google.com/*",
    "*://*.gmail.com/*",
    "*://*.twitter.com/*",
    "*://*.github.com/*",
    "*://*.behance.net/*",
    "*://*.cakeresume.com/*"
  ],
  "action": {
      "default_title": "NBI ATS",
      "default_icon": "jecho.png",
      "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
      "resources": ["jecho.png"],
      "matches": ["<all_urls>"]
  }],
  "manifest_version": 3
}
